https://leetcode.com/problems/missing-number/description/

//CHATGPT

Input : int
Keyword : NOT SURE
Constraints : 1 <= n <= 10^4

//Simple Arithmetic approach
//TC = O(n)
//SC = O(1)
class Solution {
    public int missingNumber(int[] nums) {
        int n = nums.length;
        int expectedSum = n * (n+1)/2;
        int actualSum = 0;
        for(int num : nums){
            actualSum += num;
        }
        int missingNum = expectedSum - actualSum;
        return missingNum;
    }
}

//XOR Approach
//TC = O(n)
//SC = O(1)
class Solution {
    public int missingNumber(int[] nums) {
        int missing = nums.length;
        for(int i = 0 ; i < nums.length; i++){
            missing = missing ^ i ^ nums[i];
        }
        return missing;
    }
}
//Eg: nums = [0, 1, 3, 4]
//n = 4
//missing = 4 ^ (0 ^ 1 ^ 2 ^ 3) ^ (0 ^ 1 ^ 3 ^ 4)
//missing = (0 ^ 0) ^ (1 ^ 1) ^ (3 ^ 3) ^ (4 ^ 4) ^ 2
//missing = 0 ^ 0 ^ 0 ^ 0 ^ 2 = 2

//x ^ x = 0 (XOR of a number with itself is zero)
//x ^ 0 = x (XOR of a number with zero is the number)
