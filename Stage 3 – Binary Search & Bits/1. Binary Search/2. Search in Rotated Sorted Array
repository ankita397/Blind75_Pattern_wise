

//Bruteforce - traverse the array and search for target. Return its index if found or return -1;
//TC = O(n)
//SC = O(1)
class Solution {
    public int search(int[] nums, int target) {
        int index = -1;
        for(int i = 0; i < nums.length; i++){
            if(nums[i] == target){
                index = i;
            }
        }
        return index;
    }
}
